<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <ScrollPane>
            <content>
                <TableView fx:id="table" prefHeight="600.0" prefWidth="800.0">
                    <columns>
                        <TableColumn prefWidth="100.0" text="Type" />
                        <TableColumn prefWidth="300.0" text="Nom" />
                        <TableColumn prefWidth="300.0" text="Date" />
                    </columns>
                </TableView>
            </content>
        </ScrollPane>
    </center>
</BorderPane>


